        sh:severity sh:Violation;
		sh:message "Savybės dcat:servesDataset reikšmės tipas privalo būti dcat:Dataset"@lt
    ], [
        sh:nodeKind sh:Literal ;
        sh:path dct:description ;
        sh:severity sh:Violation;
		sh:message "Savybės dct:description reikšmės tipas privalo būti rdfs:Literal"@lt
    ], [
        sh:class dct:RightsStatement ;
        sh:maxCount 1 ;
        sh:path dct:accessRights ;
        sh:severity sh:Violation;
		sh:message "Savybės dct:accessRights reikšmės tipas privalo būti dct:RightsStatement"@lt
    ], [
