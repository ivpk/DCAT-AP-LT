#-------------------------------------------------------------------------
# The shapes in this file cover all classes in DCAT-AP-LT.
#
# 
#-------------------------------------------------------------------------

:Agent_Shape
    a sh:NodeShape ;
    sh:name "Agent"@en ;
	sh:name "Agentas"@lt ;
    sh:property [
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path foaf:name ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta bent viena savybės foaf:name reikšmė, reikšmės tipas privalo būti rdfs:Literal"@lt

    ], [
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:path dct:type ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta bent viena savybės dct:type, reikšmės tipas privalo būti skos:Concept"@lt
    ], [
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:identifier ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta ne daugiau nei viena savybės dct:identifier reikšmė, reikšmės tipas privalo būti rdfs:Literal"@lt
    ] ;
    sh:targetClass foaf:Agent .

:CatalogRecord_Shape
    a sh:NodeShape ;
    sh:name "Catalogue Record"@en ;
	sh:name "Katalogo Įrašas"@lt ;
    sh:property [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:node :DcatResource_Shape ;
        sh:path foaf:primaryTopic ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta viena savybės foaf:primaryTopic reikšmė, jos tipas dcat:Resource"@lt
    ], [
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path dct:modified ;
        sh:severity sh:Violation ;
        sh:shape :DateOrDateTimeDataType_Shape;
		sh:message "Privalo būti pateikta viena savybės dct:modified reikšmė, jos tipas privalo būti rdfs:Literal"@lt
    ], [
        sh:class dct:Standard ;
        sh:maxCount 1 ;
        sh:path dct:conformsTo ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta ne daugiau nei viena savybės dct:conformsTo, jos tipas privalo būti dct:Standard"@lt
    ], [
        sh:maxCount 1 ;
        sh:node :DateOrDateTimeDataType_Shape ;
        sh:path dct:issued ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta ne daugiau nei viena savybės dct:issued reikšmė, jos tipas privalo būti rdfs:Literal"@lt
    ], [
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:path adms:status ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta ne daugiau nei viena savybės adms:status reikšmė, reikšmės tipas privalo būti skos:Concept"@lt
    ], [
        sh:class dct:LinguisticSystem ;
        sh:path dct:language ;
        sh:severity sh:Violation;
		sh:message " Savybės dct:language reikšmės tipas privalo būti dct:LinguisticSystem"@lt
    ], [
        sh:class dcat:CatalogRecord ;
        sh:maxCount 1 ;
        sh:path dct:source ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta ne daugiau nei viena savybės dct:source reikšmė, reikšmės tipas privalo būti dcat:CatalogRecord"@lt
    ], [
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation;
		sh:message "Savybės dct:title reikšmės tipas privalo būti rdfs:Literal"@lt
    ], [
        sh:nodeKind sh:Literal ;
        sh:path dct:description ;
        sh:severity sh:Violation;
		sh:message "Savybės dct:description reikšmės tipas privalo būti rdfs:Literal"@lt
    ] ;
    sh:targetClass dcat:CatalogRecord .

:Checksum_Shape
    a sh:NodeShape ;
    sh:name "Checksum"@en ;
	sh:name "Kontrolinė suma"@lt ;
    sh:property [
        sh:in (<http://spdx.org/rdf/terms#checksumAlgorithm_md2>
               <http://spdx.org/rdf/terms#checksumAlgorithm_md4>
               <http://spdx.org/rdf/terms#checksumAlgorithm_md5>
               <http://spdx.org/rdf/terms#checksumAlgorithm_md6>
               <http://spdx.org/rdf/terms#checksumAlgorithm_sha1>
               <http://spdx.org/rdf/terms#checksumAlgorithm_sha224>
               <http://spdx.org/rdf/terms#checksumAlgorithm_sha256>
               <http://spdx.org/rdf/terms#checksumAlgorithm_sha384>
               <http://spdx.org/rdf/terms#checksumAlgorithm_sha512>) ;       
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path spdx:algorithm ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta viena savybės algoritmas - spdx:algorithm reikšmė, jos tipas privalo būti vienas iš <http://spdx.org/rdf/terms#checksumAlgorithm_md2>,  <http://spdx.org/rdf/terms#checksumAlgorithm_md4>, <http://spdx.org/rdf/terms#checksumAlgorithm_md5>, <http://spdx.org/rdf/terms#checksumAlgorithm_md6>, <http://spdx.org/rdf/terms#checksumAlgorithm_sha1>, <http://spdx.org/rdf/terms#checksumAlgorithm_sha224>, <http://spdx.org/rdf/terms#checksumAlgorithm_sha256>, <http://spdx.org/rdf/terms#checksumAlgorithm_sha384>, <http://spdx.org/rdf/terms#checksumAlgorithm_sha512>"@lt
    ], [
        sh:datatype xsd:hexBinary ;
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path spdx:checksumValue ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta viena savybės dct:checksumValue reikšmė, jos tipas privalo būti xsd:hexBinary"@lt
    ] ;
    sh:targetClass spdx:Checksum .

