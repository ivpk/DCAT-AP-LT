    ], [
        sh:class cpsv:Rule ;
        sh:path cpsv:follows ;
        sh:severity sh:Violation;
		sh:message "Savybės dct:follows reikšmės tipas privalo būti cpsv:Rule"@lt
    ], [
        sh:class dct:LicenseDocument ;
        sh:maxCount 1 ;
        sh:path dct:licence ;
        sh:severity sh:Violation;
		sh:message "Privalo būti pateikta daugiausia viena savybė dct:licence reikšmė, kurios tipas privalo būti dct:LicenseDocument"@lt
		
    ];
