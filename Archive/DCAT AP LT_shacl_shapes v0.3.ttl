:Distribution_Shape
    a sh:NodeShape ;
    sh:name "Distribution"@en ;
	sh:name "Pateiktis"@lt ;
    sh:property [
        sh:minCount 1 ;
        sh:nodeKind sh:BlankNodeOrIRI;
