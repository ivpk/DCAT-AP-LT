:DateOrDateTimeDataType_Shape
    a sh:NodeShape ;
    rdfs:comment "Date time date disjunction shape checks that a datatype property receives a date or a dateTime literal" ;
    rdfs:label "Date time date disjunction" @en;
	rdfs:label "Laiko ir datos tipas" @lt;
    sh:message "The values must be data typed as either xsd:date or xsd:dateTime"   @en;
	sh:message "Vertė privalo būti xsd:date arba xsd:dateTime tipo"   @lt;
    sh:or ([
            sh:datatype xsd:date
        ]
        [
            sh:datatype xsd:dateTime
        ]
    ) .

:DcatResource_Shape
    a sh:NodeShape ;
    rdfs:comment "the union of Catalog, Dataset and DataService"@en ;
	rdfs:comment "Katalogas, Duomenų rinkinys ir Duomenų paslauga"@lt ;
    rdfs:label "dcat:Resource" ;
    sh:message "The node is either a Catalog, Dataset or a DataService"@en ; 
	sh:message "Ši reikšmė gali būti Katalogas, Duomenų rinkinys arba Duomenų paslauga"@lt ;
    sh:or ([
            sh:class dcat:Catalog
        ]
        [
            sh:class dcat:Dataset
        ]
        [
            sh:class dcat:DataService
        ]
    ) .










